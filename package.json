{
  "name": "angular-jest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "npm run build:lib:prod && npm run build:app:prod",
    "build:app:prod": "ng build --configuration production",
    "build:lib:prod": "ng build base-components --configuration production",
    "build-storybook": "ng build base-components && ng run base-components:build-storybook",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --coverage",
    "storybook": "ng run base-components:storybook",
    "storybook:dev": "ng run base-components:storybook --port 6006",
    "build:lib": "ng build base-components",
    "watch:lib": "ng build base-components --watch",
    "prestart": "npm run build:lib",
    "dev": "concurrently \"npm run watch:lib\" \"npm run storybook\" \"npm start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.6",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.6",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "base-components": "file:dist/base-components",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^19.0.0",
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.0",
    "@chromatic-com/storybook": "^3.2.6",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^8.6.9",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-onboarding": "^8.6.9",
    "@storybook/angular": "^8.6.9",
    "@storybook/blocks": "^8.6.9",
    "@storybook/test": "^8.6.9",
    "@testing-library/angular": "^17.3.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.1.2",
    "jasmine-core": "~5.6.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.3",
    "ng-packagr": "^19.2.0",
    "storybook": "^8.6.9",
    "typescript": "~5.7.2"
  }
}
